<p id="notice"><%= notice %></p>

<h1>Listing Transaccions</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>External</th>
      <th>Record</th>
      <th>Batch</th>
      <th>Payload</th>
      <th>Ejecucion</th>
      <th>Status</th>
      <th>Tipo transaccion</th>
      <th>Source</th>
      <th>Target</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transaccions.each do |transaccion| %>
      <tr>
        <td><%= transaccion.id %></td>
        <td><%= transaccion.external_id %></td>
        <td><%= transaccion.record_id %></td>
        <td><%= transaccion.batch_id %></td>
        <td><%= transaccion.payload %></td>
        <td><%= transaccion.ejecucion %></td>
        <td><%= transaccion.status %></td>
        <td><%= transaccion.tipo_transaccion %></td>
        <td><%= transaccion.source_id %></td>
        <td><%= transaccion.target_id %></td>
        <td><%= link_to 'Show', transaccion %></td>
        <td><%= link_to 'Edit', edit_transaccion_path(transaccion) %></td>
        <td><%= link_to 'Destroy', transaccion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transaccion', new_transaccion_path %>
